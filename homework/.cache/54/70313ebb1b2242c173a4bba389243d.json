\r\n    width: calc(100%/3);\r\n    height: 30px;\r\n    position: relative;\r\n}\r\n\r\n.filter__main {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    user-select: none;\r\n    height: 17px;\r\n    overflow: hidden;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.filter__main__summary {\r\n    outline: none;\r\n    font-weight: 600;\r\n    font-size: 14px;\r\n    line-height: 17px;\r\n}\r\n\r\n/* вопрос: нужно ли с учетом общей категории */\r\n\r\n.filter__main__category_li {\r\n    border: none;\r\n    display: block;\r\n}\r\n\r\n\r\nsummary {\r\n    display: block;\r\n    outline: none;\r\n}\r\n\r\nsummary::-webkit-details-marker {\r\n    display: none;\r\n}\r\n\r\n.filter__main[open] {\r\n    background: #ffff;\r\n    box-shadow: 6px 4px 35px rgba(0, 0, 0, 0.21);\r\n    width: 360px;\r\n    height: 531px;\r\n    z-index: 2;\r\n    top: -13px;\r\n    left: -16px;\r\n    padding-top: 13px;\r\n    padding-left: 16px;\r\n    transition: height 0.3s ease-in;\r\n}\r\n\r\n.filter__main[open] .filter__main__summary_top {\r\n    color: #EF5B70;\r\n    transition: color 0.2s ease-in;\r\n}\r\n\r\n\r\n.filter__main[open] .filter__main__icon {\r\n    background-image: url(img/main_filter_pink.svg);\r\n}\r\n\r\n.filter__main__category__summary {\r\n    border-left: 5px solid #EF5B70;\r\n    border-bottom: 1px solid #EBEBEB;\r\n    outline: none;\r\n    font-size: 14px;\r\n    line-height: 17px;\r\n    padding: 11px;\r\n}\r\n\r\n.filter__main__category[open] .filter__main__category__summary {\r\n    color: #EF5B70;\r\n    transition: color 0.1s ease-in-out;\r\n}\r\n\r\n.filter__main__summary_top {\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.filter__main__icon {\r\n    margin-left: 11px;\r\n    Width: 15px;\r\n    Height: 10px;\r\n    background-image: url(img/main_filter_icon.svg);\r\n}\r\n\r\n\r\n\r\n\r\n/* filtered featured items */\r\n\r\n.featured_items_catalague {\r\n    padding-bottom: 96.5px;\r\n}\r\n\r\n.featured_items__catalague__filters {\r\n    padding-top: 51px;\r\n    padding-bottom: 65px;\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.featured_items__catalague__filters_wrapper {\r\n    display: flex;\r\n    width: 100%;\r\n    position: relative;\r\n}\r\n\r\n.main_filter {\r\n    display: flex;\r\n    position: absolute;\r\n    left: 0;\r\n}\r\n\r\n.main_filter_text {\r\n    font-weight: 600;\r\n    font-size: 14px;\r\n    line-height: 17px;\r\n    color: #000000;\r\n    margin-right: 11px;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.featured_items__catalague__filters_group {\r\n    display: flex;\r\n    align-self: center;\r\n    justify-content: space-around;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    font-size: 14px;\r\n    line-height: 17px;\r\n    font-weight: 400;\r\n    color: #6F6E6E;\r\n}\r\n\r\n.featured_items__catalague__filters_group_icon {\r\n    margin-left: 5px;\r\n    margin-right: 20px;\r\n    color: #6F6E6E;\r\n}\r\n\r\n.featured_items__numbers {\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n\r\n.page-link {\r\n    font-weight: 300;\r\n    font-size: 16px;\r\n    line-height: 19px;\r\n    color: #EBEBEB;\r\n}\r\n\r\n.pointer_arrow {\r\n    color: black;\r\n}\r\n\r\n/* /////////////////////////////////// */"},"lineCount":null}},"error":null,"hash":"d9ee4549d9b401bc6665dd53bf8990e8","cacheData":{"env":{}}}