source":"registration.css","name":null,"original":{"line":184,"column":0},"generated":{"line":179,"column":0}}],"sources":{"registration.css":"@import \"../homework/style.css\";\r\n@import \"../homework/index.css\";\r\n@import \"../homework/product.css\";\r\n@import \"../homework/catalogue.css\";\r\n\r\n/* REGISTRATION.HTML STYLES */\r\n\r\n\r\n\r\n.registration_main__content {\r\n    padding-top: 64px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    padding-bottom: 96px;\r\n}\r\n\r\n.registration_main__content__left {\r\n    margin-right: 128.5px;\r\n}\r\n\r\n\r\n\r\n.registration_main__content__left__form__text {\r\n    font-weight: 300;\r\n    font-size: 16px;\r\n    line-height: 19px;\r\n    color: #222222;\r\n    padding-bottom: 20px;\r\n    text-transform: capitalize;\r\n}\r\n\r\n.registration_main__content__left__form__input {\r\n    width: 100%;\r\n    max-width: 360px;\r\n    height: 45px;\r\n    margin-bottom: 20px;\r\n    background: #FFFFFF;\r\n    border: 1px solid #A4A4A4;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: flex-start;\r\n    padding-left: 16px;\r\n    font-weight: 300;\r\n    font-size: 13px;\r\n    line-height: 16px;\r\n    color: #B1B1B1;\r\n}\r\n\r\n.registration_main__content__left__form__radiobox {\r\n    display: flex;\r\n    align-items: center;\r\n    padding-top: 11.5px;\r\n    padding-bottom: 33px;\r\n}\r\n\r\n.registration_main__content__left__form__input_radio {\r\n    border: 1px solid #A4A4A4;\r\n    width: 17px;\r\n    height: 17px;\r\n    margin-right: 10px;\r\n}\r\n\r\n\r\n.radio_text {\r\n    display: block;\r\n    font-weight: 300;\r\n    font-size: 11px;\r\n    line-height: 13px;\r\n    color: #000000;\r\n    text-transform: capitalize;\r\n    margin-right: 20px;\r\n}\r\n\r\n.registration_main__content__left__form__text_small {\r\n    margin-top: -4.5px;\r\n    padding-bottom: 39px;\r\n    font-weight: 300;\r\n    font-size: 13px;\r\n    line-height: 16px;\r\n    color: #B1B1B1;\r\n}\r\n\r\n.registration_main__content__left__button {\r\n    background: #F16D7F;\r\n    width: 100%;\r\n    Width: 167px;\r\n    Height: 50px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n.registration_main__content__left__button_text {\r\n    font-size: 14px;\r\n    line-height: 17px;\r\n    color: #FFFFFF;\r\n    margin-right: 20px;\r\n    padding-top: 22px;\r\n    margin-bottom: 21px;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.registration_main__content__right__list {\r\n    font-weight: 300;\r\n    font-size: 24px;\r\n    line-height: 29px;\r\n    color: #000000;\r\n    list-style: none;\r\n}\r\n\r\n.registration_main__content__right__list_desc {\r\n    margin-top: 22px;\r\n    margin-bottom: 21px;\r\n}\r\n\r\n.registration_main__content__list_li {\r\n    margin-bottom: 16px;\r\n}\r\n\r\n.registration_main__content__list_li:before {\r\n    content: \"\\f00c\";\r\n    font-family: FontAwesome;\r\n    display: inline-block;\r\n    margin-left: 0;\r\n    width: 20px;\r\n    height: 16px;\r\n    margin-right: 23px;\r\n}\r\n\r\n/* ADAPTIVE VERSIONS */\r\n\r\n@media (max-width: 1024px) {\r\n\r\n    .registration_main__content {\r\n        padding-top: 40px;\r\n    }\r\n\r\n    .registration_main__content__left {\r\n        margin-right: 22px;\r\n        width: 50%;\r\n    }\r\n\r\n    .registration_main__content__right {\r\n        width: 50%;\r\n    }\r\n\r\n    .registration_main__content__right__list {\r\n        font-weight: 300;\r\n        font-size: 16px;\r\n        line-height: 19px;\r\n    }\r\n\r\n    .registration_main__content__right__list_desc {\r\n        margin-top: 32px;\r\n    }\r\n\r\n}\r\n\r\n@media (max-width: 667px) {\r\n\r\n    .top_info_catalogue__content {\r\n        flex-direction: row;\r\n        justify-content: center;\r\n    }\r\n\r\n    .registration_main__content {\r\n        flex-direction: column;\r\n    }\r\n\r\n    .registration_main__content__left,\r\n    .registration_main__content__right {\r\n        width: 100%;\r\n    }\r\n\r\n    .registration_main__content__left__form__radiobox {\r\n        padding-bottom: 38.5px;\r\n    }\r\n\r\n    .registration_main__content__right {\r\n        padding-top: 40px;\r\n    }\r\n\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"261300e211f603cb88bb890c7a28b403","cacheData":{"env":{}}}